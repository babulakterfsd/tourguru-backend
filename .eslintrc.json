{
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "no-multiple-empty-lines": "warn",
    "no-unexpected-multiline": "warn",
    "no-unreachable": "error",
    "no-trailing-spaces": "warn",
    "default-case": "error",
    "generator-star-spacing": "error",
    "global-require": "warn",
    "no-await-in-loop": "warn",
    "no-const-assign": "error",
    "no-dupe-args": "warn",
    "no-dupe-keys": "warn",
    "no-duplicate-case": "warn",
    "func-names": "off",
    "no-duplicate-imports": "warn",
    "no-empty": "warn",
    "no-empty-function": "warn",
    "no-lonely-if": "warn",
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "require-yield": "warn",
    "vars-on-top": "warn",
    "no-tabs": "warn",
    "wrap-iife": "warn",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "endOfLine": "auto"
      }
    ]
  },
  "plugins": ["prettier"]
}
